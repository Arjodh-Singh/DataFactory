{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfarj"
		},
		"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
			"type": "string",
			"defaultValue": "https://storagedevarj.blob.core.windows.net/"
		},
		"Copytrgiggger_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/1f640edc-bb0e-4c94-9b67-882c9a36fe3c/resourceGroups/Dev-Prod-Analysis/providers/Microsoft.Storage/storageAccounts/storagedevarj"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfarj_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_properties_typeProperties_serviceEndpoint": {
						"value": "[parameters('AzureBlobStorage1_properties_typeProperties_serviceEndpoint')]"
					},
					"Copytrgiggger_properties_typeProperties_scope": {
						"value": "[parameters('Copytrgiggger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}